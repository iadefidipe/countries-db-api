 map-get($breakpoints-up, $size )) {\r\n        @content;\r\n    }\r\n}\r\n\r\n@mixin breakpoint-down($size) {\r\n\r\n    @media (max-width: map-get($breakpoints-down, $size )) {\r\n        @content;\r\n    } \r\n}","src\\scss\\_global.scss":"body {\r\n    max-width: 100%;\r\n    min-height: 100vh;\r\n    background-color: var(--clr-bg);\r\n    color: var(--clr-text);\r\n    font-size: rem(16);\r\n    font-family: $font-nunito-san;\r\n    font-weight: $weight-light;\r\n    box-shadow: 0 4px 4px var(--clr-bg) ;\r\n\r\n}\r\n\r\n\r\n.main-container{\r\n    \r\n    padding: rem(30) rem(70);\r\n    \r\n    @include breakpoint-down(medium){\r\n        padding: rem(30);\r\n    }\r\n    @include breakpoint-down(small){\r\n        padding: rem(20);\r\n    }\r\n\r\n}\r\n\r\n.hidden{\r\n    display: none;\r\n}","src\\scss\\_header.scss":"// header\r\n\r\n.header{\r\n    display: flex;\r\n    justify-content: space-between;\r\n    align-items: center;\r\n    padding: rem(30) rem(70);\r\n    background-color: var(--clr-elements);\r\n    box-shadow: 0 0 4px var(--clr-shadow) ;\r\n    \r\n    @include breakpoint-down(medium){\r\n        padding: rem(30);\r\n    }\r\n    @include breakpoint-down(small){\r\n        padding: rem(20);\r\n    }\r\n\r\n\r\n    &-theme-controller{\r\n        \r\n\r\n        &--button{\r\n            display: flex;\r\n            justify-content: space-between;\r\n            align-items: center;\r\n            gap: rem(5);\r\n            cursor: pointer;\r\n            background: none;\r\n            border: none;\r\n            color: var(--clr-text);\r\n            // font-weight: $weight-medium;\r\n        }\r\n    }\r\n}\r\n\r\n","src\\scss\\_main.scss":".preference,\r\n.search-input, .btn-back{\r\n  display: flex;\r\n  align-items: center;\r\n}\r\n\r\n.preference {\r\n  justify-content: space-between;\r\n\r\n  & > *{\r\n    box-shadow: 0 0 4px var(--clr-shadow) ;\r\n\r\n  }\r\n\r\n  @include breakpoint-down(small) {\r\n    flex-direction: column;\r\n    align-items: unset;\r\n    gap: rem(20);\r\n  }\r\n\r\n  .search-input {\r\n    padding: rem(10) 0 rem(10) rem(20);\r\n    background-color: var(--clr-elements);\r\n    border-radius: rem(5);\r\n    min-width: 30%;\r\n    gap: rem(10);\r\n\r\n    &-field::placeholder {\r\n      color: var(--clr-text);\r\n    }\r\n\r\n    @include breakpoint-down(small) {\r\n      min-width: 100%;\r\n    }\r\n  }\r\n\r\n  input {\r\n    width: 100%;\r\n  }\r\n\r\n  .filter {\r\n    position: relative;\r\n    width: rem(200);\r\n\r\n    & > * {\r\n      background-color: var(--clr-elements);\r\n      padding: rem(10);\r\n      border-radius: rem(5);\r\n    }\r\n\r\n    &-select {\r\n      display: flex;\r\n      align-items: center;\r\n      justify-content: space-between;\r\n      gap: rem(10);\r\n    }\r\n\r\n    &-options {\r\n      position: absolute;\r\n      top: rem(50);\r\n      left: 0;\r\n      right: 0;\r\n\r\n      p{\r\n          padding: rem(10);\r\n      }\r\n\r\n      p:hover{\r\n        background-color: var(--clr-bg);\r\n\r\n      }\r\n    }\r\n  }\r\n\r\n  .btn-back{\r\n    background-color: var(--clr-elements);\r\n    padding: rem(10) rem(30);\r\n    border-radius: rem(5);\r\n    gap: rem(20);\r\n    display: none;\r\n  }\r\n}\r\n","src\\scss\\_countries-card.scss":"\r\n\r\n.card-container{\r\n\r\n    margin-top: rem(50);\r\n    display: flex;\r\n    gap: rem(40);\r\n    flex-wrap: wrap;\r\n\r\n    .country-card{\r\n        flex: 1;\r\n        min-width: rem(250);\r\n        background-color: var(--clr-elements);\r\n        border-radius:  rem(5);\r\n        transition: transform 500ms ease-in-out;\r\n    box-shadow: 0 0 4px var(--clr-shadow) ;\r\n\r\n\r\n        &:hover{\r\n            transform: translateY(-10px);\r\n        }\r\n    }\r\n\r\n    .country-image{\r\n        img{\r\n            border-radius:  rem(5) rem(5) 0 0;\r\n        }\r\n    }\r\n\r\n    .country-description{\r\n        padding: rem(30);\r\n    }\r\n\r\n}","src\\scss\\_countries-detail.scss":"\r\n.contry-details-container,.country-borders{\r\n    display: flex;\r\n    align-items: center;\r\n}\r\n\r\n\r\n.contry-details-container{\r\n    \r\n    gap: rem(100);\r\n\r\n    @include breakpoint-down(small){\r\n        flex-direction: column;\r\n        align-items: flex-start;\r\n\r\n    }\r\n\r\n    & > *{\r\n        flex: 1;\r\n    }\r\n    .country-details, .country-props{\r\n        display: flex;\r\n        flex-direction: column;\r\n    }\r\n\r\n    .country-details{\r\n\r\n        gap: rem(20);\r\n        \r\n\r\n        \r\n\r\n        .properties{\r\n            display: flex;\r\n            justify-content: space-between;\r\n\r\n            @include breakpoint-down (small){\r\n                flex-direction: column;\r\n                gap: rem(20);\r\n            }\r\n        }\r\n\r\n        .country-props{\r\n            gap: rem(10);\r\n        }\r\n\r\n        .country-borders, .borders{\r\n            gap: rem(10);\r\n            \r\n\r\n        }\r\n\r\n        .country-borders{\r\n            @include breakpoint-down (small){\r\n                flex-direction: column;\r\n                gap: rem(20);\r\n                align-items: flex-start;\r\n            }\r\n        }\r\n\r\n\r\n\r\n            .borders{\r\n                display: flex;\r\n\r\n\r\n                &>*{\r\n                    background-color: var(--clr-elements);\r\n    box-shadow: 0 0 4px var(--clr-shadow) ;\r\n\r\n                    padding: rem(5);\r\n                    border-radius: rem(5);\r\n                }\r\n            }\r\n        \r\n    }\r\n    .country-details p > span, .country-borders p {\r\n        font-weight: $weight-medium;\r\n    }\r\n\r\n}","src\\scss\\_spinner.scss":".spinner-container {\r\n    display: grid;\r\n    place-items: center;\r\n    place-content: center;\r\n    height: rem(300);\r\n    width: 100%;\r\n    \r\n}\r\n.spinner {\r\n    &__spin {\r\n        text-align: center;\r\n        animation: loader 500ms ease-in-out infinite ;\r\n    }\r\n    i {\r\n        font-size: rem(100);\r\n    }\r\n}\r\n\r\n@keyframes loader {\r\n    0% {transform: rotateZ(0deg);}\r\n    100% {transform: rotateZ(360deg);}\r\n}"},"lineCount":null}},"error":null,"hash":"30fceee01d01bf9db26cf39538744559","cacheData":{"env":{}}}